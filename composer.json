{
    "name": "marketo-api/marketo-rest-client",
    "description": "A PHP client for the Marketo.com REST API",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/neclimdul/oauth2-marketo.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2.1",
        "guzzlehttp/guzzle-services": "^1.1.3",
        "softonic/guzzle-oauth2-middleware": "^1.2",
        "kristenlk/oauth2-marketo": "master"
    },
    "replace": {
        "marketo-api/marketo-rest-client": "0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "CSD\\Marketo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CSD\\Marketo\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Daniel Chesterton"
        }
    ]
}
